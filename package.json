{
  "name": "decentraland-editor",
  "version": "0.1.0",
  "description": "Collaborative, real time edition of parcels, with IPFS uploading",
  "main": "dist/aframe-inspector.min.js",
  "scripts": {
    "build": "webpack --progress --colors",
    "remotebuild": "NODE_PATH=/tmp/editor/node_modules webpack --progress --colors",
    "dev": "docker-compose up",
    "dist": "npm run dist:max && npm run dist:min",
    "dist:max": "cross-env AFRAME_DIST=true npm run build",
    "dist:min": "cross-env NODE_ENV=production AFRAME_DIST=true npm run build",
    "lint": "npm run lintfile src/",
    "lint:css": "stylelint src/css/main.css",
    "lintfile": "node --harmony node_modules/.bin/eslint",
    "start": "node server.js",
    "test": "webpack --config webpack.test.config.js"
  },
  "repository": "decentraland/editor",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "clipboard": "^2.0.0",
    "cors": "^2.8.4",
    "decentraland-commons": "^3.4.0",
    "express": "^4.16.2",
    "express-http-proxy": "^1.0.7",
    "immutable": "^3.8.2",
    "invariant": "^2.2.2",
    "lodash.debounce": "^4.0.6",
    "node-fetch": "^2.1.2",
    "prop-types": "^15.6.0",
    "query-string": "^6.0.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-file-reader-input": "^1.1.4",
    "react-modal": "^3.0.4",
    "react-redux": "^5.0.6",
    "react-select": "^1.0.0-rc.10",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.1",
    "simple-peer": "^9.0.0",
    "tape": "^4.8.0",
    "tape-run": "^4.0.0",
    "tempy": "^0.2.1",
    "three": "^0.91.0",
    "web3": "^1.0.0-beta.26",
    "web3-provider-engine": "git+https://github.com/decentraland/provider-engine.git#releases",
    "webpack-tape-run": "0.0.7"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.26.0",
    "babel-eslint": "^6.1.0",
    "babel-jest": "^16.0.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-class-properties": "^6.10.2",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.x",
    "eslint-config-standard": "^5.3.1",
    "eslint-plugin-promise": "^1.3.2",
    "eslint-plugin-react": "^5.2.2",
    "eslint-plugin-standard": "^1.3.2",
    "gh-pages": "^0.11.0",
    "gm": "^1.23.0",
    "jest": "^16.0.1",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "micro-dom": "scenevr/micro-dom",
    "object-assign": "^4.1.0",
    "open": "0.0.5",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.9.1",
    "prettier-standard": "^7.0.3",
    "react-test-renderer": "^15.3.2",
    "shx": "^0.1.2",
    "standard": "^10.0.3",
    "style-loader": "^0.13.1",
    "stylelint": "^8.0.0",
    "stylelint-config-standard": "^17.0.0",
    "stylelint-order": "^0.6.0",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  },
  "keywords": [
    "3d",
    "aframe",
    "decentraland",
    "ipfs",
    "ethereum",
    "editor",
    "inspector",
    "three.js",
    "tool",
    "unity",
    "vr",
    "virtualreality",
    "webvr",
    "wysiwyg"
  ]
}
